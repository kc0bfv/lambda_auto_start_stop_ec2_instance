variable "environ_tag" {
  default = "nicename"
}

variable "instance" {
  default = "i0000000"
}

variable "region" {
  default = "us-east-1"
}

# See the AWS eventbridge documentation
variable "eventbridge_expression" {
  default = "cron(1 * ? * * *)"
}

# Date format "yyyy-mm-ddThh:mm-hh:mm"
variable "base_start_time" {
  default = "2022-01-01T14:00-05:00"
}

# Format "h hours"
variable "run_time" {
  default = "6 hours"
}

# Format "d days"
variable "rate" {
  default = "14 days"
}
